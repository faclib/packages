[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
	hideDotFiles = dotGitOnly

[remote "origin"]
	url = https://github.com/faclib/packages.git
	fetch = +refs/heads/*:refs/remotes/origin/*
	pushurl = git@github.com:faclib/packages.git
	push = gh-pages:refs/heads/gh-pages

[branch "gh-pages"]
	remote = origin
	merge = refs/heads/gh-pages
[user]
	name = builder
	email = localhost
